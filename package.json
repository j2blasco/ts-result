{
  "name": "@j2blasco/ts-result",
  "version": "0.1.10",
  "description": "Result Monad for TypeScript",
  "author": "j2blasco",
  "license": "MIT",
  "keywords": [
    "typescript",
    "result",
    "monad",
    "error-handling"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j2blasco/ts-result.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:dts",
    "build:js": "tsup",
    "build:dts": "tsc -p tsconfig.build.json && node scripts/build-maps.cjs",
    "clean": "shx rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "type": "module",
  "devDependencies": {
    "@j2blasco/ts-pipe": "^0.0.8",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.3.2",
    "tsup": "^8.5.0",
    "typescript": "~5.7.2"
  }
}
