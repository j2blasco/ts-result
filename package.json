{
  "name": "@j2blasco/ts-result",
  "version": "0.1.2",
  "description": "Result Monad for TypeScript",
  "author": "j2blasco",
  "license": "MIT",
  "keywords": [
    "typescript",
    "result",
    "monad",
    "error-handling"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/j2blasco/ts-crud"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "shx rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "type": "module",
  "devDependencies": {
    "@j2blasco/ts-pipe": "^0.0.4",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.3.2",
    "tsup": "^8.5.0",
    "typescript": "~5.7.2"
  }
}
